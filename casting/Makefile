# This file was create by hand to compile the project in this folder.

# Declaration of variables
CC = g++
CXXFLAGS = -w -std=c++17
 
# File names
SOURCES = $card *.cpp)
OBJECTS = ullcast.o 
EXEC = ullcast

# Main target
$(EXEC): $(OBJECTS)
	$(CC) $(CXXFLAGS) -o $(EXEC) $(OBJECTS)
 
# To remove generated files
clean:
	rm -f $(EXEC) $(OBJECTS)
